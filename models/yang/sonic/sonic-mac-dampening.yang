module sonic-mac-dampening {
    namespace "http://github.com/Azure/sonic-mac-dampening";
    prefix sfdb;
    yang-version 1.1;

    organization
        "SONiC";

    contact
        "SONiC";

    description
        "SONIC MAC Dampening";

    revision 2020-09-14 {
        description
            "Initial revision.";
    }

    container sonic-mac-dampening {

        container MAC_DAMPENING{
            list MAC_DAMPENING_LIST{
                key "config";
                leaf config {
                    type enumeration {
                        enum config;
                    }
                    description
                        "mac_dampening DB key.";
                }
                leaf threshold {
                    type uint8 {
                        range "5..100" {
                            error-message "threshold value out of range";
                        }
                    }
                }
            }
        }
    }
}
